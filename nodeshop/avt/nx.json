{
  "npmScope": "avt",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "admin": {
      "tags": []
    },
    "admin-e2e": {
      "tags": [],
      "implicitDependencies": ["admin"]
    },
    "ng-shop": {
      "tags": []
    },
    "ng-shop-e2e": {
      "tags": [],
      "implicitDependencies": ["ng-shop"]
    },
    "orders": {
      "tags": []
    },
    "products": {
      "tags": []
    },
    "ui": {
      "tags": []
    },
    "users": {
      "tags": []
    }
  }
}
